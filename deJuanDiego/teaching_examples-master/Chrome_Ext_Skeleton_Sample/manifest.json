{
  "name": "My Chrome Extension Skeleton Sample",
  "version": "0.1",
  "description": "Chrome Extension Skeleton Sample for training purposes",
  "author" : "Juan Diego Pérez Jiménez",
  "permissions": [
    "tabs"  
    ],
  "browser_action": {
      "default_title": "My Chrome Extension Skeleton Sample",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_scripts" : [{
   "matches" : ["http://*/*"],
   "css" : ["css/popup.css"],
   "js" : ["js/jquery.min.js","js/popup.js"]
  }
  ],
  "content_security_policy": "default-src 'self'; connect-src http://www.zaragoza.es"
  
}
